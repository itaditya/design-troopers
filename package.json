{
  "name": "design-troopers",
  "version": "2.1.4",
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "files": [
    "dist/*"
  ],
  "sideEffects": [
    "*.css"
  ],
  "license": "MIT",
  "scripts": {
    "gen:tokens": "node scripts/generateTokens.js > src/tokens/tokens.json",
    "gen:tokens:dev": "nodemon --exec 'yarn gen:tokens' --watch src/tokens --ignore src/tokens/tokens.json",
    "gen:iconslist": "node scripts/generateIconsList.js > iconsList.json",
    "copy:tokens": "cp tokens.json dist/tokens.json",
    "docs": "styleguidist server",
    "docs:dev": "nodemon --exec 'yarn docs' --watch styleguide.config.js --watch package.json",
    "docs:build": "styleguidist build",
    "docs:deploy": "yarn docs:build && surge -p styleguide -d https://design-troopers.surge.sh",
    "build": "yarn build:es && yarn build:lib",
    "build:lib": "webpack --mode production",
    "build:es": "rollup -c",
    "test": "jest",
    "test:dev": "yarn test --watch",
    "test:debug": "node --inspect-brk $(npm bin)/jest --runInBand --watch",
    "cy:open": "cypress open --config testFiles='*.spec.js'",
    "cy:openvisual": "cypress open --config testFiles='*.visualspec.js'",
    "cy:openpercy": "percy exec --  cypress open --config testFiles='*.percyspec.js'",
    "cy:run": "cypress run",
    "cy:runpercy": "percy exec --  cypress run",
    "prepublishOnly": "yarn build && yarn gen:tokens && yarn copy:tokens",
    "postpublish": "yarn docs:deploy",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@percy/cypress": "^2.1.1",
    "@storybook/addon-essentials": "^6.1.7",
    "@storybook/addon-links": "^6.1.7",
    "@storybook/addons": "^6.1.7",
    "@storybook/react": "^6.1.7",
    "@testing-library/react": "^9.1.0",
    "autoprefixer": "^9.6.1",
    "axe-core": "^3.3.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-react-docgen": "^4.1.0",
    "birla": "^1.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "cypress": "^3.4.1",
    "cypress-image-snapshot": "^3.1.1",
    "flow-bin": "^0.105.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^0.8.0",
    "packagebind": "^0.3.3",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-docgen-external-proptypes-handler": "^1.0.3",
    "react-docgen-imported-proptype-handler": "^1.0.4",
    "react-dom": "^16.9.0",
    "react-is": "^16.12.0",
    "react-styleguidist": "^9.1.14",
    "react-svg-loader": "^3.0.3",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^5.1.1",
    "storybook-chromatic": "^3.5.5",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "material-svg-react-icons": "3",
    "tailwindcss": "^1.9.0",
    "tailwindcss-transitions": "^2.0.1"
  }
}
